{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": false,
    "declaration": true,
    "removeComments": false,
    "noImplicitAny": true,
    "outDir": "built/"
  },
  "exclude": [
    "built",
    "node_modules",
    "testapp",
    "website",
    "scripts",
    "globals.ts",
    "globals.d.ts",
    "exampleTypescript",
    "spec"
  ]
}
