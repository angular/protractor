{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "declaration": true,
    "removeComments": false,
    "noImplicitAny": true,
    "outDir": "built/",
    "types": [
      "jasmine", "node",
      "chalk", "glob", "minimatch",
      "minimist", "optimist", "q",
      "selenium-webdriver"
    ]
  },
  "exclude": [
    "built",
    "node_modules",
    "testapp",
    "website",
    "scripts",
    "exampleTypescript",
    "spec"
  ]
}
