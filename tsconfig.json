{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": false,
    "declaration": true,
    "removeComments": false,
    "noImplicitAny": true,
    "outDir": "built/",
    "types": [
      "jasmine", "node",
      "chalk", "glob", "minimatch",
      "minimist", "optimist", "q"
    ]
  },
  "exclude": [
    "built",
    "node_modules",
    "testapp",
    "website",
    "scripts",
    "globals.ts",
    "globals.d.ts",
    "exampleTypescript",
    "typings/globals",
    "spec"
  ]
}
