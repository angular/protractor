{
  "name": "Protractor tester",
  "version": "1.0",
  "manifest_version": 2,
  "description": "An extension to test protractor selectors.",
  "icons": {
    "16": "images/icon-16.png",
    "128": "images/icon-128.png"
  },
  "background": {
    "scripts": [
      "scripts/background.js"
    ]
  },
  "browser_action": {
    "default_icon": "images/icon-38.png",
    "default_popup": "popup/popup.html"
  },
  "devtools_page": "devtoolsBackground.html",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}
